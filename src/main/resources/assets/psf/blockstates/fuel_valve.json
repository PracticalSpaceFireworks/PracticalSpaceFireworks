{
  "multipart": [
    {
      "when": { "part": "bottom" },
      "apply": { "model": "psf:fuel_valve_bottom" }
    },
    {
      "when": { "part": "top" },
      "apply": { "model": "psf:fuel_valve_top" }
    },
    {
      "when": { "part": "none" },
      "apply": { "model": "psf:fuel_valve_none" }
    },
    {
      "when": { "part": "both" },
      "apply": { "model": "psf:fuel_valve_both" }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "north": "true" },
          { "part": "both", "north": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_side", "uvlock": true }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "north_east": "true" },
          { "part": "both", "north_east": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_corner", "uvlock": true }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "east": "true" },
          { "part": "both", "east": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_side", "uvlock": true, "y": 90 }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "south_east": "true" },
          { "part": "both", "south_east": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_corner", "uvlock": true, "y": 90 }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "south": "true" },
          { "part": "both", "south": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_side", "uvlock": true, "y": 180 }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "south_west": "true" },
          { "part": "both", "south_west": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_corner", "uvlock": true, "y": 180 }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "west": "true" },
          { "part": "both", "west": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_side", "uvlock": true, "y": 270 }
    },
    {
      "when": {
        "OR": [
          { "part": "top", "north_west": "true" },
          { "part": "both", "north_west": "true" }
        ]
      },
      "apply": { "model": "psf:fuel_tank_corner", "uvlock": true, "y": 270 }
    }
  ]
}
